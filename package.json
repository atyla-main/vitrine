{
  "name": "vitrine",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "ajv": "^6.4.0",
    "ajv-keywords": "^3.2.0",
    "bootstrap": "^3.3.7",
    "cross-fetch": "^2.2.2",
    "font-awesome": "^4.7.0",
    "husky": "^0.14.3",
    "i": "^0.3.6",
    "i18next": "^11.3.1",
    "i18next-browser-languagedetector": "^2.2.0",
    "i18next-xhr-backend": "^1.5.1",
    "lint-staged": "^7.0.5",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.2.2",
    "npm": "^6.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.2",
    "react-flags-select": "^1.1.8",
    "react-https-redirect": "^1.0.10",
    "react-i18next": "^7.6.1",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.0",
    "react-scripts": "1.1.4",
    "react-scrollable-anchor": "^0.6.1",
    "react-select": "^1.2.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.3.0",
    "written-number": "^0.8.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./src/styles --include-path ./node_modules src/ -o src/ --watch",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./src/styles --include-path ./node_modules src/ -o src/ --watch",
    "precommit": "lint-staged",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
